{
  "name": "DFOXA WPAPI",
  "require": {
    "flc/alidayu": "^2.0",
    "respect/validation": "^1.1",
    "jenssegers/agent": "^2.5",
    "houdunwang/dir": "^1.0",
    "zhuzhichao/ip-location-zh": "^2.4",
    "bupt1987/html-parser": "^3.0",
    "wapmorgan/file-type-detector": "^1.1"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  },
  "autoload": {
    "psr-4": {
      "account\\oauth\\": [
        "libs/account/oauth"
      ],
      "account\\sign\\": [
        "libs/account/sign"
      ],
      "account\\token\\": [
        "libs/account/token",
        "libs/account/token/modes"
      ],
      "gateway\\": "libs/gateway",
      "tools\\": "libs/tools",
      "tools\\sms\\": "libs/tools/sms",
      "tools\\email\\": [
        "libs/tools/email",
        "libs/tools/email/lib"
      ],
      "tools\\sql\\": "libs/tools/sql",
      "tools\\logs\\":[
        "libs/tools/logs",
        "libs/tools/logs/set",
        "libs/tools/logs/get",
        "libs/tools/logs/update"
      ],
      "media\\": "libs/media",
      "posts\\": "libs/posts",
      "tests\\": "libs/tests"
    }
  }
}
